{
  "name": "@gulujs/eslint-config",
  "version": "0.5.1",
  "description": "GuluJS's ESLint config",
  "main": "index.js",
  "files": [
    "js/*",
    "ts/*",
    "rules/*"
  ],
  "scripts": {
    "lint": "eslint --ext .js .",
    "lint:fix": "eslint --fix --ext .js .",
    "unused:js": "eslint-find-rules --unused ./js/index.js",
    "unused:node": "eslint-find-rules --unused ./js/node.js",
    "unused:ts": "eslint-find-rules --unused ./ts/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gulujs/eslint-config.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "gulujs",
    "sytleguide"
  ],
  "author": "jKey Lu <jkeylu@gmail.com> (https://github.com/jkeylu)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gulujs/eslint-config/issues"
  },
  "homepage": "https://github.com/gulujs/eslint-config#readme",
  "peerDependencies": {
    "eslint": "^8.27.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.27.0",
    "eslint-find-rules": "^4.1.0",
    "eslint-plugin-node": "^11.1.0",
    "typescript": "^4.8.4"
  }
}
